{
  "name": "mdn-express-generator-tutorial",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www",
    "serverstart": "DEBUG=mdn-express-generator-tutorial:* npm run devstart",
    "git": "git add . && git commit && git push origin $(git rev-parse --abbrev-ref HEAD) && echo '\n' && echo $(tput setaf 2)'commited: ' $(tput setaf 1)$(date) && echo '\n'",
    "startdb": "brew services start mongodb-community@5.0 && mongosh",
    "stopdb": "brew services stop mongodb-community@5.0"
  },
  "dependencies": {
    "async": "^3.2.2",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "luxon": "^2.3.0",
    "mongoose": "^6.1.5",
    "morgan": "~1.9.1"
  }
}
